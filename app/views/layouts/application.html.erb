<!DOCTYPE html>
<html>
<head>
  <title>Feedbackhero</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>

  
</head>
<body>
	<!-- start: Header -->
	<div class="navbar">
		<div class="navbar-inner">
			<div class="container-fluid">
				<a class="btn btn-navbar" data-toggle="collapse" data-target=".top-nav.nav-collapse,.sidebar-nav.nav-collapse">
					<span class="icon-bar"></span>
					<span class="icon-bar"></span>
					<span class="icon-bar"></span>
				</a>

				<!--<a class="brand" href="index.html"><span>Feedback SuperHero</span></a>-->
				<!--<%#= link_to "Feedback SuperHero", root_path, :class => 'brand' %>-->
				<%= link_to root_path do %>
  								<a class="brand"><span>Feedback SuperHero</span></a>
							<% end %>	
				

				<!-- start: Header Menu -->
				<div class="nav-no-collapse header-nav">
					<ul class="nav pull-right">
						<!-- start: User Dropdown -->
						<li class="dropdown">
							<a class="btn dropdown-toggle" data-toggle="dropdown" href="#">
								<i class="halflings-icon white user"></i> <%= current_user.email %>
								<span class="caret"></span>
							</a>
							<ul class="dropdown-menu">
								<li><a href="#"><i class="halflings-icon white user"></i> <%= link_to 'Edit profile', edit_user_registration_path %></a></li>
								<li><a href="login.html"><i class="halflings-icon white off"></i> <%= link_to "Logout", destroy_user_session_path, method: :delete %></a></li>
							</ul>
						</li>
						<!-- end: User Dropdown -->

					</ul>
				</div>	
				<!-- end: Header Menu -->
			</div>
		</div>	
	</div>
	<!-- end: Header -->

	<!-- start: Main Content -->




	<div class="container-fluid">
	<div class="row-fluid">

		<!-- start: Main Menu -->
			<div id="sidebar-left" class="span1">
				<div class="nav-collapse sidebar-nav">
					<ul class="nav nav-tabs nav-stacked main-menu">
						
						<li><%= link_to myview_index_path do %>
  								<i class="fa-icon-dashboard"></i><span class="hidden-tablet"> My View</span>
							<% end %>
						</li>	
						<li><%= link_to feedbacks_path do %>
  								<i class="fa-icon-envelope"></i><span class="hidden-tablet"> Feedback</span>
							<% end %>
						</li>	
						<li><%= link_to requestedfeedbacks_path do %>
  								<i class="fa-icon-edit"></i><span class="hidden-tablet"> Requests</span>
							<% end %>
						</li>	
						<li><%= link_to badges_path do %>
  								<i class="fa-icon-dashboard"></i><span class="hidden-tablet"> Badges</span>
							<% end %>
						</li>	
						<li><%= link_to user_badges_path do %>
  								<i class="fa-icon-dashboard"></i><span class="hidden-tablet"> User Badges</span>
							<% end %>
						</li>
						<% if can? :update, User %>
							<li><%= link_to users_path do %>
	  								<i class="fa-icon-dashboard"></i><span class="hidden-tablet"> Users</span>
								<% end %>
							</li>	
							<li><%= link_to roles_path do %>
	  								<i class="fa-icon-dashboard"></i><span class="hidden-tablet"> Roles</span>
								<% end %>
							</li>	
						<% end %>	

						<li><a href="infrastructure.html"><i class="fa-icon-hdd"></i><span class="hidden-tablet"> Infrastructure</span></a></li>
						<li><a href="messages.html"><i class="fa-icon-envelope"></i><span class="hidden-tablet"> Messages</span></a></li>
						<li><a href="tasks.html"><i class="fa-icon-tasks"></i><span class="hidden-tablet"> Tasks</span></a></li>
						<li><a href="ui.html"><i class="fa-icon-eye-open"></i><span class="hidden-tablet"> UI Features</span></a></li>
						<li><a href="widgets.html"><i class="fa-icon-dashboard"></i><span class="hidden-tablet"> Widgets</span></a></li>
						<li>
							<a class="dropmenu" href="#"><i class="fa-icon-folder-close-alt"></i><span class="hidden-tablet"> Dropdown</span></a>
							<ul>
								<li><a class="submenu" href="submenu.html"><i class="fa-icon-file-alt"></i><span class="hidden-tablet"> Sub Menu 1</span></a></li>
								<li><a class="submenu" href="submenu.html"><i class="fa-icon-file-alt"></i><span class="hidden-tablet"> Sub Menu 2</span></a></li>
								<li><a class="submenu" href="submenu.html"><i class="fa-icon-file-alt"></i><span class="hidden-tablet"> Sub Menu 3</span></a></li>
							</ul>	
						</li>
						<li><a href="form.html"><i class="fa-icon-edit"></i><span class="hidden-tablet"> Forms</span></a></li>
						<li><a href="chart.html"><i class="fa-icon-list-alt"></i><span class="hidden-tablet"> Charts</span></a></li>
					</ul>
				</div>
			</div>
			<!-- end: Main Menu -->


			<!-- start: Content -->
			<div id="content" class="span11">
				<div class="row-fluid sortable">	<!-- just a row -->
					<div class="box span12">
						<%= yield %>
					</div>
				</div>
			</div>
			<!-- end: Content -->	


	</div><!--/.fluid-row-->	
	</div><!--/.fluid-container-->		

	

	<div id="user_nav">
		<% if user_signed_in? %>
			Logged in as <strong><%= current_user.email %></strong>.
			<%= link_to 'Edit profile', edit_user_registration_path %> |
			<%= link_to "Logout", destroy_user_session_path, method: :delete %>
		<% else %>
		  	<%= link_to "Sign up", new_user_registration_path %> |
		  	<%= link_to "Login", new_user_session_path %>
		<% end %>
	</div>

		<p class="notice"><%= notice %></p>
       	<p class="alert"><%= alert %></p>

	


 		
</body>
</html>
