<h1>Users#index</h1>
<p>Find me in app/views/users/index.html.erb</p>


	<table>
		<tr>
				<th>First Name</th>
				<th>Last Name</th>
				<th>Email</th>
				<th>Role</th>
				<th>Role Id</th>
		</tr>

<% @users.each do |user| %>
	
		<tr>
				<td><%= user.first_name %></td>
				<td><%= user.last_name %></td>
				<td><%= user.email %></td>
				<td><%= user.role.role_name %></td>
				<td><%= user.role_id %></td>
		</tr>
	</table>
<% end %>	
