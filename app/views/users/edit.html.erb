<h1>Edit User</h1>




<%= form_for @user do |f| %>
	<p>
		<%= f.label :first_name %><br/>
		<%= f.text_field :first_name %><br/>
		<%= f.label :last_name %><br/>
		<%= f.text_field :last_name %><br/>
		<%= f.label :email %><br/>
		<%= f.text_field :email %><br/>
		<%= f.label "Role" %><br/>
		<%= f.collection_select :role_id, Role.all, :id , :role_name %>
	</p>

	<p>
		<%= f.submit "Update" %>
	</p>	


<% end %>